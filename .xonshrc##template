def _yadm(args, stdin=None):
    if args[0] == "edit":
        nvim @(f"{args[1]}##template")
    else:
        yadm @(args)
aliases["psn"] = _yadm
aliases["resource"] = lambda args, stdin=None: $(source @(f"/home/{$(whoami)}/.xonshrc"))
aliases["brew"] = lambda args, stdin=None: $(/home/linuxbrew/.linuxbrew/Homebrew/bin/brew.sh @(args))
